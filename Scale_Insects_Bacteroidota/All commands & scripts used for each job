# Flye v2.9
/apps/unit/HusnikU/Flye/bin/flye --pacbio-raw /bucket/HusnikU/Sequencing_data/Coccoidea_external/SRR17283552_PacBioSequel.fastq --out-dir /flash/HusnikU/Jinyeong/Flye/ACLA --meta --scaffold --threads 128
/apps/unit/HusnikU/Flye/bin/flye --pacbio-raw /bucket/HusnikU/Sequencing_data/Coccoidea_external/SRR26067558_PacBioHiFi.fastq --out-dir /flash/HusnikU/Jinyeong/Flye/COCA --meta --scaffold --threads 128

# Pear v0.9.11
/apps/unit/HusnikU/PEAR/pear-0.9.11-linux-x86_64/bin/pear -f /path/to/read1.fastq.gz -r /path/to/read2.fastq.gz -o /path/to/output

# Blastn
/apps/unit/HusnikU/ncbi-blast-2.11.0+/bin/blastn -task megablast -db /bucket/HusnikU/Databases/nt/nt -query /path/to/metagenome_assembly/scaffolds.fasta -outfmt '6 qseqid staxids bitscore std sscinames ssk-ingdoms stitle' -culling_limit 5 -evalue 1e-25 -num_threads 16 -out /path/to/output/scaffolds_vs_nt.blast

# Blobtools v1.1.1
for Spades assembly
/apps/unit/HusnikU/blobtools/blobtools create -i /path/to/metagenome_assembly/scaffolds.fasta -y sapdes -t /path/to/blastn/scaffolds_vs_nt.blast -o /path/to/output

for Flye assembly
/apps/unit/HusnikU/blobtools/blobtools create -i /path/to/metagenome_assembly/scaffolds.fasta -cov /path/to/contigs_stats.txt -t /path/to/blastn/scaffolds_vs_nt.blast -o /path/to/output

/apps/unit/HusnikU/blobtools/blobtools plot -i /path/to/blobtools.blobDB.json -r superkingdom -o /path/to/output
/apps/unit/HusnikU/blobtools/blobtools plot -i /path/to/blobtools.blobDB.json -r phylum -o /path/to/output
/apps/unit/HusnikU/blobtools/blobtools plot -i /path/to/blobtools.blobDB.json -r order -o /path/to/output

/apps/unit/HusnikU/blobtools/blobtools view -i /path/to/blobtools.blobDB.json -o /path/to/output

# Metabat2 v2.12.1
metabat2 -i /path/to/metagenome_assembly/scaffolds.fasta -a /path/to/scaffolds_depth.txt -o /path/to/output

# Dastool v1.1.6
for converting fasta to tsv files
Fasta_to_Contig2Bin.sh -e /path/to/assembly_metabat.fasta > /path/to/my_contigs2bin.tsv

for running Dastool
DAS_Tool -i /path/to/my_contigs2bin.tsv -c /path/to/assembly_metabat.fasta -o /path/to/output -l metabat --write_bin_evals --write_bins

# Pilon v1.24
for Illumina assembly
java -jar /apps/unit/HusnikU/pilon-1.24.jar --genome /path/to/assembly.fasta --frags /path/to/sorted_bam_file --output output_prefix --outdir /path/to/output

for Pacbio assembly - polished by short-reads
java -jar /apps/unit/HusnikU/pilon-1.24.jar --genome /path/to/assembly.fasta --pacbio /path/to/sorted_bam_file --output output_prefix --outdir /path/to/output

