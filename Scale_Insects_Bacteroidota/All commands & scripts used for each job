# FastQC v0.11.9
fastqc /path/to/*.fastq.gz -o /path/to/output

# Fastp v0.21
/apps/unit/HusnikU/fastp/0.21/fastp \
-i /path/to/read1.fastq.gz -o /path/to/read1_trimmed.fastq.gz \
-I /path/to/read2.fastq.gz -O /path/to/read2_trimmed.fastq.gz

# Phyloflash v3.4
phyloFlash.pl -read1 /path/to/read1.fastq.gz -read2 /path/to/read2.fastq.gz \
-CPUs 96 -lib <Sample_CODE> -almosteverything -zip -dbhome /path/to/138.1 -readlength 150

## Symbiont phylogeny

# Barrnap v3
/apps/unit/HusnikU/barrnap/bin/barrnap \
--incseq /path/to/metagenome_assembly/scaffolds.fasta --outseq /path/to/output

# Mafft v7
mafft --auto /path/to/*.fasta > /path/to/output

# Trimal v1.4
trimal -in /path/to/*.fasta -out /path/to/output.fasta -htmlout /path/to/output.html -automated1

# Orthofinder v2.5.4
/apps/unit/HusnikU/OrthoFinder/orthofinder -f /path/to/input_dir -M msa -T iqtree

# BUSCO v5.1.3
busco --force --in /path/to/*.fasta --lineage_dataset bacteroidetes_odb10 --out /path/to/output --mode genome --cpu 64

# BuscoPro2Phylo
Rscript /home/j/jinyeong-choi/sbatch_buscoProt2Phylo/buscoProt2Phylo_pipeline.R /home/j/jinyeong-choi/sbatch_buscoProt2Phylo/buscoProt2Phylo_main.R /home/j/jinyeong-choi/sbatch_buscoProt2Phylo/buscoProt2Phylo_conf_Bacteroidota_symbionts.R

buscoProt2Phylo_conf.R
buscoProt2Phylo_main.R
buscoProt2Phylo_pipeline.R
buscoProt2Phylo.sh

*Original scripts
https://github.com/CongLiu37/Notes/blob/main/pipelines/buscoProt2Phylo_conf.R
https://github.com/CongLiu37/Notes/blob/main/pipelines/buscoProt2Phylo_main.R
https://github.com/CongLiu37/Notes/blob/main/pipelines/buscoProt2Phylo_pipeline.R


